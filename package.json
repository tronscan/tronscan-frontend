    "scripts": {
        "start": "node scripts/start.js",
        "start:https": "HTTPS=true node scripts/start.js",
        "start:testnet": "NET=testnet API_URL=https://testapi.tronscan.org yarn start",
        "build:shasta": "GENERATE_SOURCEMAP=false API_URL=https://api.shasta.tronscan.org node scripts/build.js",
        "build": "NODE_ENV=production GENERATE_SOURCEMAP=false node scripts/build.js",
        "desktop:build": "GENERATE_SOURCEMAP=false API_URL=https://api.tronscan.org DESKTOP=true yarn run build",
        "desktop:prepare": "rm -rf ./build-native && mkdir build-native && cp desktop/* build-native/ && cp -r ./build/* build-native/",
        "desktop:build:win": "electron-packager ./build-native Tronscan --platform=win32 --out=build-desktop --asar --overwrite",
        "desktop:build:linux": "electron-packager ./build-native Tronscan --platform=linux --out=build-desktop --overwrite",
        "desktop:build:mac": "electron-packager ./build-native Tronscan --platform=darwin --out=build-desktop  --overwrite",
        "desktop:start": "DEV=true electron ./desktop",
        "desktop:start:test": "DEV=true electron ./desktop",
        "desktop:build:full": "yarn desktop:build && yarn run desktop:prepare && yarn desktop:build:win && yarn desktop:build:linux && yarn desktop:build:mac",
        "test": "node scripts/test.js --env=jsdom",
        "deploy": "gh-pages -d build",
        "analyz": "NODE_ENV=production npm_config_report=true npm run build"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*.d.ts"
        ],
        "setupFiles": [
            "react-app-polyfill/jsdom"
        ],
        "setupFilesAfterEnv": [],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
            "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
        ],
        "testEnvironment": "jest-environment-jsdom-fourteen",
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "modulePaths": [],
        "moduleNameMapper": {
            "^react-native$": "react-native-web",
            "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ],
        "watchPlugins": [
            "jest-watch-typeahead/filename",
            "jest-watch-typeahead/testname"
        ]
    },
    "devDependencies": {
        "@babel/plugin-proposal-decorators": "^7.4.4",
        "babel-plugin-add-module-exports": "^1.0.2",
        "cache-loader": "^4.1.0",
        "extract-text-webpack-plugin": "^3.0.2",
        "glob": "^7.1.6",
        "glob-all": "^3.2.1",
        "purgecss-webpack-plugin": "^2.1.0"
    },
    "description": "<h1 align=\"center\">tronscan<br></h1>",
